<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

	<servers>
		<server>
			<id>overpass-nexus</id>
		</server>
		<server>
			<id>overpass-snapshots</id>
			<username>deployment</username>
			<password>deployment</password>
		</server>
		<server>
			<id>overpass-releases</id>
			<username>deployment</username>
			<password>deployment</password>
		</server>
	</servers>

	<mirrors>
		<mirror>
			<id>overpass-nexus</id>
			<mirrorOf>*</mirrorOf>
			<url>http://129.152.138.20/nexus/content/groups/public/</url>
		</mirror>
	</mirrors>
	
	<profiles>
		<profile>
			<id>overpass-nexus</id>
			<repositories>
				<repository>
					<id>central</id>
					<name>Maven Repository Switchboard</name>
					<url>http://129.152.138.20/nexus/content/groups/public/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>central</id>
					<url>http://129.152.138.20/nexus/content/groups/public/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</pluginRepository>
			</pluginRepositories>
			<properties>
				<downloadSources>true</downloadSources>
				<downloadJavadocs>true</downloadJavadocs>
			</properties>
		</profile>
	</profiles>
	
	<activeProfiles>
		<activeProfile>overpass-nexus</activeProfile>
	</activeProfiles>	

</settings>
